<% provide(:title, 'All users') %>
<h1>All users</h1>
<div>
<%= form_tag users_path, :method => 'get' do %>
  <p>
    <%= text_field_tag :search, params[:search] , class: "input-large search-query"%>
    <%= submit_tag "Search", :name => nil, class:"btn btn-primary" %>
  </p>
<% end %>
</div>

<ul class="users">
      <%= render @users %>
<%= will_paginate @users %>
</ul>
